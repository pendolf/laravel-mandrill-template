{
    "name": "addgod/laravel-mandrill-template",
    "description": "Using Mandrill templates in Laravel.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6|^7|^8",
        "illuminate/contracts": "^6|^7|^8",
        "illuminate/notifications": "^6|^7|^8",
        "mandrill/mandrill": "dev-master",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "Addgod\\MandrillTemplate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addgod\\MandrillTemplate\\MandrillTemplateServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pendolf/mandrill-api-php"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
